{
  "extends": [
    "@windwalker-io/unicorn-next/tsconfig.js.json"
  ],
  "include": [
    "./src/**/*.ts",
    "./src/types/globals.d.ts",
    "./src/**/*.vue",
    "./src/**/*.js",
  ],
  "exclude": [
    "node_modules/**/*"
  ],
  "types": [
    "reflect-metadata"
  ],
  "compilerOptions": {
      "allowJs": true,
      "skipLibCheck": true,
      "paths": {
          "~luna/*": ["./src/*"]
      }
  }
}
